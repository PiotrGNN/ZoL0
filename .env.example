# API Keys - Zastąp swoimi własnymi kluczami
BYBIT_API_KEY=your_api_key_here
BYBIT_API_SECRET=your_api_secret_here
BYBIT_USE_TESTNET=true  # Ustaw na true dla testowego API, false dla produkcyjnego

# Konfiguracja środowiska
#Konfiguracja Flask
FLASK_APP=main.py
FLASK_ENV=development
PORT=5000
FLASK_SECRET_KEY=twoj_losowy_klucz_tajny

# Konfiguracja ByBit API
#BYBIT_API_KEY=TwojKluczAPI
#BYBIT_API_SECRET=TwojSekretAPI
#BYBIT_USE_TESTNET=true

# Parametry limitowania API
API_MIN_INTERVAL=10.0
API_MAX_CALLS_PER_MINUTE=5
API_CACHE_TTL_MULTIPLIER=10.0


# Konfiguracja logowania
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL

# Parametry handlowe
RISK_LEVEL=low  # low, medium, high
MAX_POSITION_SIZE=0.1  # 10% portfela
ENABLE_AUTO_TRADING=false  # true/false

# Konfiguracja API i rate limitingu
#API_RATE_LIMIT=true  # Włącza inteligentne limitowanie zapytań API
#API_CACHE_ENABLED=true  # Włącza buforowanie odpowiedzi API