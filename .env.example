# API Bybit - Konfiguracja
BYBIT_API_KEY=your_api_key_here
BYBIT_API_SECRET=your_api_secret_here
BYBIT_TESTNET=true
# BYBIT_USE_TESTNET jest przestarzały, używaj BYBIT_TESTNET zamiast niego

# Tryby aplikacji
DEBUG=True
LOG_LEVEL=INFO
IS_PRODUCTION=False
USE_SIMULATED_DATA=false

# VPN i Proxy
#USE_PROXY=False
#PROXY_TYPE=socks5h  # socks5h lub http
#PROXY_HOST=127.0.0.1
#PROXY_PORT=1080
#SSH_HOST=your_vps_ip
#SSH_USER=root
#SSH_PORT=22

# Konfiguracja Flask
FLASK_APP=main.py
FLASK_ENV=development
PORT=5000
FLASK_SECRET_KEY=your_secret_key_here

# Konfiguracja systemu
ENABLE_AUTO_TRADING=False
RISK_LEVEL=low
MAX_POSITION_SIZE=1.0

# Application Settings (retained from original)
# These settings are not directly conflicting and might be useful
CACHE_TTL=300
MAX_WORKERS=4
RATE_LIMIT_RETRY=3

