run = "python3 main.py"
entrypoint = "main.py"
modules = ["python-3.10:v18-20230807-322e88b"]
hidden = [".pythonlibs"]

[nix]
channel = "stable-23_05"

[env]
PYTHONPATH = "$PYTHONPATH:${REPL_HOME}"
PYTHONUNBUFFERED = "1"

[[ports]]
localPort = 5000
externalPort = 80

[languages.python3]
pattern = "**/*.py"

[languages.python3.languageServer]
start = ["pylsp"]

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 31819671
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 main.py"
