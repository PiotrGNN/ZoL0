
run = "python main.py"
modules = ["python-3.10:v18-20230807-322e88b"]

hidden = [".pythonlibs"]

[nix]
channel = "stable-23_05"

[env]
PYTHONPATH = "$PYTHONPATH:${REPL_HOME}"
PYTHONUNBUFFERED = "1"
PYTHONOPTIMIZE = "1"
PYTHONHASHSEED = "42"
PYTHONTRACEMALLOC = "0"
PYTHONASYNCIODEBUG = "0"

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["python", "main.py"]
deploymentTarget = "cloudrun"
ignorePorts = false

[server]
host = "0.0.0.0"
port = 5000

[port]
5000 = "http"

[[ports]]
localPort = 5000
externalPort = 80
exposeLocalhost = true

[[ports]]
localPort = 8080
externalPort = 8080

[interpreter]
command = ["python3"]

[runOnStart]
command = "python init_project.py"

[packager]
language = "python3"

[packager.features]
packageSearch = true
guessImports = true

[languages]
[languages.python3]
pattern = "**/*.py"
[languages.python3.languageServer]
start = "pylsp"

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 31819671
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"
