
entrypoint = "main.py"
modules = ["python-3.10:v18-20230807-322e88b"]

hidden = [".pythonlibs"]

[nix]
channel = "stable-23_05"

[env]
PYTHONPATH = "${REPL_HOME}"

[deployment]
run = ["python3", "main.py"]
deploymentTarget = "cloudrun"

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[languages]

[languages.python3]
pattern = "**/*.py"

[languages.python3.languageServer]
start = "pylsp"

[server]
host = "0.0.0.0"
port = 5000

[port]
5000 = "http"

[[ports]]
localPort = 5000
externalPort = 80

[interpreter]
command = [
  "python3"
]

[packager]
language = "python3"
ignoredPackages = []

[packager.features]
packageSearch = true
guessImports = true
enabledForHosting = false

[runOnStart]
pattern = "**/*.py"
command = "python3"

[workflows]
runButton = "Run"
