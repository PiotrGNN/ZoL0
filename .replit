run = "python main.py"
language = "python3"
entrypoint = "main.py"

hidden = [".pythonlibs"]

[env]
PYTHONPATH = "."
PORT = "5000"
HOST = "0.0.0.0"

[nix]
channel = "stable-23_11"

[deployment]
run = "python main.py"
deploymentTarget = "cloudrun"

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 31819671
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"

[[ports]]
localPort = 5000
externalPort = 80
