
run = "python3 main.py"
modules = ["python-3.10:v18-20230807-322e88b"]
hidden = [".pythonlibs"]

[nix]
channel = "stable-22_11"

[env]
PYTHONPATH = "/home/runner/${REPL_SLUG}/"
FLASK_APP = "main.py"
FLASK_ENV = "development"

[packager]
language = "python3"

[packager.features]
enabledForHosting = false
packageSearch = true
guessImports = true

[unitTest]
language = "python3"

[deployment]
run = "python3 main.py"
deploymentTarget = "cloudrun"

[languages]

[languages.python3]
pattern = "**/*.py"

[languages.python3.languageServer]
start = "pylsp"

[[ports]]
localPort = 5000
externalPort = 80

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 31819671
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 main.py"
