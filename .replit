run = "/home/runner/.pythonlibs/bin/python3 -m main"
language = "python3"
entrypoint = "main.py"

[nix]
channel = "stable-22_11"

[deployment]
run = "/home/runner/.pythonlibs/bin/python3 -m main"
deploymentTarget = "cloudrun"

[env]
PYTHONPATH = "."

[languages.python3]
pattern = "**/*.py"

[languages.python3.languageServer]
start = ["pylsp"]

[[ports]]
localPort = 5000
externalPort = 80

[workflows]
runButton = "Run"
