run = ["python", "main.py"]
entrypoint = "main.py"
hidden = [".config", "pyproject.toml", ".pylintrc", ".flake8", ".gitignore", "**/__pycache__", "**/.mypy_cache", "**/*.pyc"]
modules = ["python-3.10:v18-20230807-322e88b"]

[nix]
channel = "stable-22_11"

[env]
PYTHONPATH = "${PYTHONPATH}:${workspaceDir}"
PORT = "5000"
FLASK_APP = "main.py"
FLASK_ENV = "development"

[languages]
python = "CPython 3.10"

[languages.python]
pattern = "**/*.py"

[languages.python.languageServer]
start = ["pylsp"]

[webview]
port = 5000

[server]
host = "0.0.0.0"
port = 5000

[port]
5000 = "http"

[[ports]]
localPort = 5000
externalPort = 80

[interpreter]
command = [
  "python3"
]

[runOnStart]
pattern = "**/*.py"
command = "python3"

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 31819671
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 main.py"