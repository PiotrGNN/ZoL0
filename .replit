PORT = "5000"
HOST = "0.0.0.0"

[nix]
channel = "stable-23_11"

[deployment]
run = "python main.py"
deploymentTarget = "cloudrun"

[workflows]
runButton = "Run"

[[ports]]
localPort = 5000
externalPort = 80
