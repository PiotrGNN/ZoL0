entrypoint = "main.py"
modules = ["python-3.10:v18-20230807-322e88b"]

hidden = [".pythonlibs"]

[nix]
channel = "stable-23_05"

[deployment]
run = ["python", "main.py"]
deploymentTarget = "cloudrun"

[env]
PYTHONPATH = "${REPL_HOME}"

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[languages]

[languages.python3]
pattern = "**/*.py"

[languages.python3.languageServer]
start = "pylsp"

[interpreter]
command = ["python3"]

[packager]
language = "python3"
ignoredPackages = ["jupyter"]

[packager.features]
packageSearch = true
guessImports = true

[server]
host = "0.0.0.0"
port = 5000

[port]
5000 = "http"

[[ports]]
localPort = 5000
externalPort = 80

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 31819671
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 main.py"
